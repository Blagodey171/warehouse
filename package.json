        "jsonwebtoken": "^8.5.1",
        "mongodb": "^4.1.3",
        "mongodb-client-encryption": "^1.2.7",
        "mongoose": "^5.12.10",
        "postcss-preset-env": "^6.7.0",
        "react": "^17.0.2",
