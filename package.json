{
    "name": "warehouse-webpack",
    "version": "1.0.0",
    "description": "warehouse for AFY",
    "main": "./dist/client/index.html",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve",
        "dev": "concurrently \"nodemon ./server/index.ts\" \"cross-env NODE_ENV=development webpack serve\"",
        "build-server-tsc": "tsc server/index.ts",
        "build": "cross-env NODE_ENV=production webpack",
        "build-dev": "cross-env NODE_ENV=development webpack --mode development",
        "pm2-dev": "concurrently \"tsc --watch server/index.ts\" \"pm2 start server/index.js --exp-backoff-restart-delay=1000 --watch --wait-ready\" \"cross-env NODE_ENV=development webpack serve\"",
        "pm2-dev:delete": "pm2 delete server/index.js",
        "pm2-prod": "pm2 start dist/server.node.js --exp-backoff-restart-delay=1000 --watch --wait-ready",
        "pm2-prod:delete": "pm2 delete dist/server.node.js"
    },
    "repository": {
        "type": "git",
        "url": "Blagodey171"
    },
    "author": "Denis P.",
    "license": "ISC",
